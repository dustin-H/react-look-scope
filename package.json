		"react-look",
		"look",
		"server-rendering",
		"node"
	],
	"author": "Dustin Hoffner",
	"license": "SEE LICENSE IN LICENSE",
	"bugs": {
		"url": "https://github.com/dustin-H/react-look-scope/issues"
	},
	"homepage": "https://github.com/dustin-H/react-look-scope#readme",
  "peerDependencies": {
    "react-look": "1.0.0-beta7"
  },
	"devDependencies": {
		"babel-cli": "^6.6.5",
		"babel-plugin-add-module-exports": "^0.1.2",
		"babel-preset-es2015": "^6.6.0"
	}
